{
  "name": "react-sip",
  "version": "0.3.2",
  "description": "React wrapper for jssip",
  "main": "./dist/index.js",
  "scripts": {
    "build:watch": "babel src -d dist --watch",
    "build": "babel src -d dist",
    "format:js": "prettier-eslint \"./src/**/*.js\" --write",
    "lint:js": "eslint src",
    "lint": "npm run lint:js",
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/callthemonline/react-sip.git"
  },
  "keywords": [
    "react",
    "sip",
    "jssip"
  ],
  "author": "callthem online (https://callthem.online/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/callthemonline/react-sip/issues"
  },
  "homepage": "https://github.com/callthemonline/react-sip#readme",
  "dependencies": {
    "jssip": "3.x",
    "prop-types": "15.x"
  },
  "peerDependencies": {
    "react": "15.x"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "prettier-eslint": "^6.4.2",
    "prettier-eslint-cli": "^4.1.1"
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs",
      "transform-class-properties"
    ],
    "presets": [
      "es2015",
      "react"
    ]
  }
}
