  },
  "keywords": [
    "react",
    "sip",
    "jssip"
  ],
  "author": "callthem online (https://callthem.online/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/callthem-online/react-sip/issues"
  },
  "homepage": "https://github.com/callthem-online/react-sip#readme",
  "dependencies": {
    "jssip": "^3.0.13",
    "prop-types": "^15.5.10"
  },
  "peerDependencies": {
    "react": "15.x"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1"
  },
  "babel": {
