{
  "name": "react-sip",
  "version": "0.1.5",
  "description": "React wrapper for jssip",
  "main": "./dist/index.js",
  "scripts": {
    "build:watch": "babel src -d dist --watch",
    "build": "babel src -d dist",
    "prepublish": "npm run build",
